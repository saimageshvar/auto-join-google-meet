{
  "manifest_version": 3,
  "name": "Auto Join",
  "version": "1.0",
  "description": "Auto Join",
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://meet.google.com/*",
        "https://chat.google.com/*",
        "https://mail.google.com/chat/*",
        "https://mail.google.com/mail/u/0/#chat/*"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}